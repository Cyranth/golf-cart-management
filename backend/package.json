{
   "name": "backend",
   "version": "1.0.0",
   "description": "",
   "main": "index.js",
   "scripts": {
      "dev": "nodemon -L --exec ts-node src/index.ts",
      "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
      "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
      "build": "tsc",
      "start": "node ./dist/src/index.js",
      "test": "jest",
      "test:watch": "jest --watch",
      "typeorm": "ts-node ./node_modules/typeorm/cli.js ",
      "migration:generate": "npm run typeorm -- migration:generate -d src/database/dataSource.ts",
      "migration:run": "npm run build && npm run typeorm -- migration:run -d src/database/dataSource.ts",
      "migration:run:no-build": "npm run typeorm -- migration:run -d src/database/dataSource.ts",
      "migration:revert": "npm run typeorm -- migration:revert -d src/database/dataSource.ts",
      "migration:create": "npm run typeorm -- migration:create"
   },
   "keywords": [],
   "author": "",
   "license": "ISC",
   "dependencies": {
      "body-parser": "1.20.2",
      "cors": "2.8.5",
      "express": "4.19.2",
      "express-rate-limit": "7.2.0",
      "pg": "8.11.5",
      "reflect-metadata": "0.2.2",
      "typeorm": "0.3.20",
      "zod": "3.23.4"
   },
   "devDependencies": {
      "@types/cors": "2.8.17",
      "@types/express": "4.17.21",
      "@types/jest": "29.5.12",
      "@types/node": "20.12.7",
      "@types/pg": "8.11.5",
      "@typescript-eslint/eslint-plugin": "7.8.0",
      "@typescript-eslint/parser": "7.8.0",
      "eslint": "8.57.0",
      "eslint-config-prettier": "9.1.0",
      "eslint-plugin-prettier": "5.1.3",
      "jest": "29.7.0",
      "jest-environment-jsdom": "29.7.0",
      "nodemon": "3.1.0",
      "prettier": "3.2.5",
      "ts-jest": "29.1.2",
      "ts-node": "10.9.2",
      "typescript": "5.4.5"
   }
}
